	<extension point="org.eclipse.ui.navigator.navigatorContent">    
            <commonWizard type="new"
                menuGroupId="2org.wso2.developerstudio.eclipse.artifact.cep"
                wizardId="org.wso2.developerstudio.eclipse.artifact.newexecution-planartifact">
                <enablement>
                    <or>
                        <adapt type="org.eclipse.core.resources.IProject">
                            <test property="org.eclipse.core.resources.projectNature"
                                value="org.wso2.developerstudio.eclipse.artifact.cep.project.nature" />
                                </adapt>
                       <adapt type="org.eclipse.jdt.core.IPackageFragment">
							<test property="org.wso2.developerstudio.projectNature"
								value="org.wso2.developerstudio.eclipse.artifact.cep.project.nature" />
						</adapt>
						<adapt type="org.eclipse.core.resources.IFolder">  
						   <test
                       forcePluginActivation="true"
                       property="org.eclipse.core.resources.name"
                       value="execution-plan">
                     </test>                        
						</adapt>
                    </or>
                </enablement>
            </commonWizard>
    </extension>
    
    <extension point="org.eclipse.ui.editors">
       <editor
             class="org.wso2.developerstudio.eclipse.artifact.cep.editor.CEPProjectEditor"
             extensions="xml"
             id="org.wso2.developerstudio.eclipse.artifact.cep.editor.CEPProjectEditor"
             name="CEPEditor">
          <contentTypeBinding
                contentTypeId="org.wso2.developerstudio.eclipse.artifact.cep.content-type.cepservice">
          </contentTypeBinding>
       </editor>
    </extension>
