<plugin>
   <extension point="org.eclipse.ui.newWizards">
         <wizard name="Execution-plan" 
         	category="org.wso2.developerstudio.eclipse.capp.project/org.wso2.developerstudio.eclipse.artifact.cep.category"
			class="org.wso2.developerstudio.eclipse.artifact.cep.execution_plan.ui.wizard.Execution_planProjectCreationWizard"
			wizardManifest="project_wizard.xml"
			id="org.wso2.developerstudio.eclipse.artifact.newexecution-planartifact"
			project="true"
			icon="icons/execution_plan-16x16.png">
			<description>Execution-plan</description>
		</wizard>
         <category
               id="org.wso2.developerstudio.eclipse.artifact.cep.execution_plan.category"
               name="Execution-plan"
               parentCategory="org.wso2.developerstudio.eclipse.capp.project">
         </category>
   </extension>
     
